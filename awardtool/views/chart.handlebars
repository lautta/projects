<html>

<head>
  <!--Load the AJAX API-->
  <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
  <script type="text/javascript">
    // Load the Visualization API and the corechart package.
    google.charts.load('current', { 'packages': ['corechart'] });

    // Set a callback to run when the Google Visualization API is loaded.
    google.charts.setOnLoadCallback(drawChart);

    // Callback that creates and populates a data table,
    // instantiates the pie chart, passes in the data and
    // draws it.
    function drawChart() {

      // Create the data table.
      var data = new google.visualization.DataTable();
      data.addColumn('string', 'Name');
      data.addColumn('number', 'awards');
      data.addRows([
        ['Jane Doe', 2],
        ['Jone Doe', 1]
      ]);

      // Set chart options
      var options = {
        'title': 'The chart of employee of month awards',
        'width': 800,
        'height': 600
      };
      var chart = new google.visualization.PieChart(document.getElementById('chart_div'));
      chart.draw(data, options);
    }
  </script>
</head>

<body>
  <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <p class="nav navbar-brand">Octans Award Tool</p>
      </div>
      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          <li><a href="/viewuser"><span class="glyphicon glyphicon-user"></span>View User</a></li>
          <li><a href="/"><span class="glyphicon glyphicon-log-out"></span>Logout</a></li>
        </ul>
      </div>
    </div>
  </nav>
  <!--Div that will hold the pie chart-->
  <div id="chart_div"></div>
  <div id="chart_div2"></div>
  <div class="container">
    <h3> Winner is Jane Doe.</h3>
  </div>

</body>

</html>
